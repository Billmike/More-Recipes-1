{
  "name": "more-recipes",
  "version": "1.0.0",
  "engines": {
    "node": "7.10.0"
  },
  "description": "A platform for sharing recipes",
  "main": "index.js",
  "scripts": {
    "build": "babel ./server -d ./build",
    "build:client": "webpack -p && cp client/public/index.html client/dist/index.html",
    "heroku-postbuild": "npm run build && npm run migrate",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "unmigrate": "node_modules/.bin/sequelize db:migrate:undo:all",
    "start": "node ./build/app",
    "start:dev": "nodemon --exec babel-node ./server/app",
    "start:client": "webpack -d && cp client/public/index.html client/dist/index.html && webpack-dev-server --inline --hot",
    "test": "set NODE_ENV=test && npm run unmigrate && npm run migrate && nyc mocha --compilers js:babel-core/register tests/test.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dheavyman/More-Recipes.git"
  },
  "keywords": [
    "API",
    "React",
    "Redux"
  ],
  "author": "Justin Nebo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dheavyman/More-Recipes/issues"
  },
  "homepage": "https://github.com/Dheavyman/More-Recipes#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.19.4",
    "morgan": "^1.9.0",
    "nodemailer": "^4.4.0",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-modal": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.7.5",
    "sequelize-cli": "^2.8.0",
    "validator": "^9.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "chai": "^4.1.2",
    "coveralls": "^2.13.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "mocha": "^3.5.0",
    "mocha-lcov-reporter": "^1.3.0",
    "node-sass": "^4.6.1",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "supertest": "^3.0.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
