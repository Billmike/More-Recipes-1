{
  "name": "more-recipes",
  "version": "1.0.0",
  "engines": {
    "node": "7.10.0"
  },
  "description": "A platform for sharing recipes",
  "main": "index.js",
  "scripts": {
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "unmigrate": "node_modules/.bin/sequelize db:migrate:undo:all",
    "start": "babel-node ./bin/www",
    "start:dev": "nodemon --exec babel-node ./bin/www",
    "test": "nyc mocha --compilers js:babel-core/register tests/test.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dheavyman/More-Recipes.git"
  },
  "keywords": [
    "API"
  ],
  "author": "Justin Nebo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dheavyman/More-Recipes/issues"
  },
  "homepage": "https://github.com/Dheavyman/More-Recipes#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.7.5",
    "sequelize-cli": "^2.8.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "chai": "^4.1.2",
    "coveralls": "^2.13.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1",
    "supertest": "^3.0.0"
  }
}
